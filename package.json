    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-mailto": "^0.4.0",
    "react-router": "^3.0.0",
    "sass-loader": "^4.0.2"
