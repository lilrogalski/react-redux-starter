{
  "name": "react-es6-webpack",
  "version": "4.0.0",
  "description": "Boilerplate for kick starting a React Project with ES6 (Babel) and Hot reloader using Webpack.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --open",
    "build": "./node_modules/.bin/cross-env BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.production.js",
    "prod": "BABEL_ENV=production node server/prod.js --progress --colors --profile",
    "lint": "DEBUG=eslint:cli-engine eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanrogalski/react-es6-webpack.git"
  },
  "keywords": [
    "react",
    "es6",
    "babel",
    "webpack"
  ],
  "author": "Ryan Rogalski (http://www.ryanrogalski.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryanrogalski/react-es6-webpack/issues"
  },
  "homepage": "https://github.com/ryanrogalski/react-es6-webpack",
  "dependencies": {
    "autoprefixer": "^9.6.0",
    "aws-sdk-webpack": "^2.18.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.6.0",
    "babel-runtime": "^6.3.19",
    "classnames": "^2.2.5",
    "compression-webpack-plugin": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^12.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "save": "^2.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.21",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.0.1",
    "css-loader": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.14.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^4.0.0",
    "postcss-loader": "^3.0.0",
    "react-transform-hmr": "^1.0.1",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  }
}
